[application]

config/name="Metroidvania"
run/main_scene="res://Menus/StartMenu.tscn"
config/icon="res://Player Icon.png"
config/windows_native_icon="res://player icon.ico"

[autoload]

Utils="*res://utils.gd"
CommonResources="*res://CommonResources.gd"
Events="*res://Events.gd"
